{
  "name": "bwh",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "format": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --pretty --noEmit",
    "pages:build": "yarn next-on-pages",
    "preview": "yarn pages:build && wrangler pages dev",
    "deploy": "yarn pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "build:worker": "opennextjs-cloudflare",
    "dev:worker": "wrangler dev --port 8771",
    "preview:worker": "yarn build:worker && yarn dev:worker",
    "deploy:worker": "yarn build:worker && wrangler deploy"
  },
  "dependencies": {
    "@as-integrations/next": "^3.2.0",
    "@headlessui/react": "2.2",
    "@prisma/client": "^6.1.0",
    "@types/nodemailer": "^6.4.17",
    "@types/react-responsive-masonry": "^2.1.3",
    "animate.css": "^4.1.1",
    "bootstrap": "^5.3.3",
    "currency.js": "^2.0.4",
    "dayjs": "^1.11.13",
    "devextreme": "23.1",
    "devextreme-react": "23.1",
    "gray-matter": "^4.0.3",
    "jose": "^5.9.6",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "mysql2": "^3.12.0",
    "next": "15.1.0",
    "nodemailer": "^6.9.16",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.7",
    "react-bootstrap-icons": "^1.11.5",
    "react-bootstrap-typeahead": "^6.3.4",
    "react-datepicker": "^7.5.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.0.1",
    "react-popper": "^2.3.0",
    "react-responsive-masonry": "^2.6.0",
    "server-only": "^0.0.1",
    "serverless-mysql": "^1.5.5",
    "swr": "^2.3.0",
    "type-graphql": "^2.0.0-rc.2",
    "uuid-wand": "^1.0.3",
    "xml2js": "^0.6.2",
    "zod": "^3.24.1"
  },
  "resolutions": {
    "@headlessui/react": "^2.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241224.0",
    "@eslint/eslintrc": "^3",
    "@opennextjs/cloudflare": "https://pkg.pr.new/@opennextjs/cloudflare@main",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-datepicker": "^7.0.0",
    "@types/react-dom": "^19",
    "@types/underscore": "^1.13.0",
    "@types/xml2js": "^0.4.14",
    "class-validator": "^0.14.1",
    "eslint": "^9",
    "eslint-config-next": "15.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prisma": "^6.1.0",
    "reflect-metadata": "^0.2.2",
    "typescript": "^5",
    "vercel": "^39.2.2",
    "wrangler": "^3.99.0"
  }
}
