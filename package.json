{
  "name": "bwh",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "format": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --pretty --noEmit",
    "pages:build": "yarn next-on-pages",
    "preview": "yarn pages:build && wrangler pages dev",
    "deploy": "yarn pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "build:worker": "opennextjs-cloudflare",
    "dev:worker": "wrangler dev --port 8771",
    "preview:worker": "yarn build:worker && yarn dev:worker",
    "deploy:worker": "yarn build:worker && wrangler deploy"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-menubar": "^1.1.4",
    "@radix-ui/react-navigation-menu": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.1",
    "@tanstack/react-table": "^8.20.6",
    "@types/nodemailer": "^6.4.17",
    "@types/react-responsive-masonry": "^2.1.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "currency.js": "^2.0.4",
    "dayjs": "^1.11.13",
    "jose": "^5.9.6",
    "lodash": "^4.17.21",
    "lucide-react": "^0.469.0",
    "next": "15.1.0",
    "nodemailer": "^6.9.16",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.7",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.0.1",
    "react-responsive-masonry": "^2.6.0",
    "recharts": "^2.15.0",
    "server-only": "^0.0.1",
    "serverless-mysql2": "^1.5.6",
    "shadcn-ui": "^0.9.4",
    "swr": "^2.3.0",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.24.1"
  },
  "resolutions": {
    "semver": "^7.6.3",
    "path-to-regexp": "^8.2.0",
    "mysql2": "^3.12.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241224.0",
    "@next/bundle-analyzer": "^15.1.3",
    "@opennextjs/cloudflare": "https://pkg.pr.new/@opennextjs/cloudflare@7654867",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/underscore": "^1.13.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9",
    "eslint-config-next": "15.1.0",
    "gray-matter": "^4.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5",
    "wrangler": "^3.99.0"
  }
}
