{
  "name": "with-typescript-eslint-jest",
  "author": "@erikdstock",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "lint": "eslint src --ext ts --ext tsx --ext js",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.7",
    "@types/reactstrap": "^8.5.0",
    "@types/underscore": "^1.10.10",
    "aws-sdk": "^2.725.0",
    "azure-devops-node-api": "^10.1.1",
    "devextreme": "^20.1.6",
    "devextreme-react": "^20.1.6",
    "dotenv": "^8.2.0",
    "moment": "^2.27.0",
    "mysql2": "^2.1.0",
    "next": "^9.5.1",
    "next-compose-plugins": "^2.2.0",
    "next-optimized-images": "^2.6.2",
    "node-sass": "^4.14.1",
    "node-fetch": "^2.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "reactstrap": "^8.5.1",
    "swr": "^0.2.3",
    "twilio": "^3.48.1",
    "underscore": "^1.10.2"
  },
  "devDependencies": {
    "@testing-library/react": "^10.0.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.5",
    "@types/react": "^16.9.27",
    "@types/testing-library__react": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-jest": "^25.2.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.3",
    "jest": "^25.2.3",
    "jest-watch-typeahead": "^0.5.0",
    "lint-staged": "^10.0.10",
    "prettier": "^2.0.2",
    "typescript": "^3.8.3"
  }
}
