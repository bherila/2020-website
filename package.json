{
  "name": "bwh",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next dev --turbopack",
    "build": "next build",
    "format": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --pretty --noEmit",
    "pages:build": "yarn next-on-pages",
    "preview": "yarn pages:build && wrangler pages dev",
    "deploy": "yarn pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "build:worker": "opennextjs-cloudflare",
    "dev:worker": "wrangler dev --port 8771",
    "preview:worker": "yarn build:worker && yarn dev:worker",
    "deploy:worker": "yarn build:worker && wrangler deploy",
    "upload:app": "rsync -ac --delete --progress --exclude='.DS_Store' .next/standalone/ bherila@web1.dal.cloudplatform.net:~/bherila.net/",
    "upload:static": "rsync -ac --delete --progress --exclude='.DS_Store' .next/static bherila@web1.dal.cloudplatform.net:~/bherila.net/_next/",
    "upload:public": "rsync -a --delete --progress public/ bherila@web1.dal.cloudplatform.net:~/bherila.net/_next/public"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
